{
  "name": "clinic-frontend",
  "version": "2.4.10",
  "type": "module",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "postbuild": "node scripts/inject-client-id-postbuild.js",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:clinic-frontend": "node dist/clinic-frontend/server/server.mjs",
    "generate:manifest": "node scripts/generate-manifest.js",
    "prebuild": "npm run generate:manifest",
    "build:masajecorporaldeportivo": "cross-env VITE_CLIENT_ID=masajecorporaldeportivo npm run build",
    "build:actifisio": "cross-env VITE_CLIENT_ID=actifisio npm run build",
    "deploy:masajecorporaldeportivo": "npm run build:masajecorporaldeportivo && cp vercel.masajecorporaldeportivo.json vercel.json.bak && cp vercel.masajecorporaldeportivo.json vercel.json",
    "deploy:actifisio": "npm run build:actifisio && cp vercel.actifisio.json vercel.json.bak && cp vercel.actifisio.json vercel.json"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^20.2.4",
    "@angular/compiler": "^20.2.4",
    "@angular/core": "^20.2.4",
    "@angular/forms": "^20.2.4",
    "@angular/platform-browser": "^20.2.4",
    "@angular/platform-browser-dynamic": "^20.2.4",
    "@angular/platform-server": "^20.2.4",
    "@angular/router": "^20.2.4",
    "@angular/service-worker": "^20.2.4",
    "@angular/ssr": "^20.2.4",
    "@capacitor/android": "^7.4.3",
    "@capacitor/cli": "^7.4.3",
    "@capacitor/core": "^7.4.3",
    "@capacitor/ios": "^7.4.3",
    "@fullcalendar/angular": "^6.1.19",
    "@fullcalendar/core": "^6.1.19",
    "@fullcalendar/daygrid": "^6.1.19",
    "@fullcalendar/interaction": "^6.1.19",
    "@fullcalendar/timegrid": "^6.1.19",
    "@supabase/supabase-js": "^2.89.0",
    "bootstrap": "^5.3.7",
    "bootstrap-icons": "^1.13.1",
    "electron-updater": "^6.6.2",
    "express": "^4.18.2",
    "moment": "^2.30.1",
    "moment-timezone": "^0.6.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.2.4",
    "@angular/cli": "^20.2.4",
    "@angular/compiler-cli": "^20.2.4",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^18.18.0",
    "cross-env": "^10.1.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.8.3"
  }
}
