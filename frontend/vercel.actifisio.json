{
  "name": "actifisio-app",
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/clinic-frontend/browser"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_CLIENT_ID": "actifisio"
  },
  "build": {
    "env": {
      "VITE_CLIENT_ID": "actifisio"
    }
  }
}
